    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Button fx:id="btnStop" mnemonicParsing="false" onAction="#btnStopOne" text="stop" />

    <Label fx:id="lblOne" />
